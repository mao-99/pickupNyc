{
  "name": "pickupnyc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "nodemon src/backend/src/server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "api-dev": "nodemon --watch ./src/backend/src/server.js"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "@react-google-maps/api": "^2.19.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "next": "14.2.4",
    "pg": "^8.12.0",
    "prisma": "^5.16.1",
    "react": "^18",
    "react-dom": "^18",
    "react-places-autocomplete": "^7.3.0"
  },
  "devDependencies": {
    "concurrently": "8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "nodemon": "^3.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
